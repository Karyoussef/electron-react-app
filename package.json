{
  "name": "cra-electron-forge-demo",
  "version": "0.1.0",
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Karim",
  "description": "my appp",
  "private": true,
  "dependencies": {
    "3": "^2.1.0",
    "-": "0.0.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^1.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "typescript": "^4.1.3",
    "typescript-dev": "^0.9.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k --success first \"SET BROWSER=none&&npm run start\" \"npm run electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
      "last 1 firefox version",
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.1.1",
    "wait-on": "^5.1.0",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "electron.react.app",
    "productName": "HackerNews",
    "copyright": "Copyright Â© 2020 Karim",
    "buildDependenciesFromSource": "false",
    "nodeGypRebuild": "false",
    "npmRebuild": "false",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "build/icon.*"
    ],
    "fileAssociations": [
      {
        "ext": ".hna",
        "name": "HNA",
        "description": "HackerNews Project"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": false
    }
  }
}
